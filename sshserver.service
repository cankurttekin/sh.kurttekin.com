[Unit]
Description=SSH TUI Portfolio Service
After=network.target

[Service]
Type=simple
User=ec2-user
WorkingDirectory=/home/ec2-user/sh.kurttekin.com
ExecStart=/home/ec2-user/sh.kurttekin.com/tuiserver
Restart=on-failure
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target 